{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: gcc build active file",
      "command": "/usr/bin/gcc",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${file}",
        "./markdown/generator.c",
        "./markdown/parser.c",
        "./lib/list.c",
        "./lib/regexp.c",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}",
        "&",
        "./${fileBasenameNoExtension}",
        "in.md"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ]
}